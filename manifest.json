{
	"manifest_version": 2,

	"name": "findlink",
	"description": "A special URI fragment causes pages to jump to a location on load.",
	"version": "1.0",

	"content_scripts": [
		{
			"js": ["node_modules/babel-core/node_modules/regenerator/runtime.js", "findlink.js"],
			"matches": ["<all_urls>"]
		}
	],

	"icons": {
		"128": "icon128.png"
	}
}
